      <bind nodeset="/data/plantData/dateBackwashed" type="date" relevant="(selected(/data/plantData/backwashed, 'yes'))"/>
      <bind nodeset="/data/plantData/coagulantCheck" type="select1" relevant="(/data/plantData/coagulantDose &gt;= 50)" required="true()" constraint="selected(., 'Yes')"/>
      <bind nodeset="/data/plantData/settledWaterTurbidityCheck" type="select1" relevant="(/data/plantData/settledWaterTurbidity &gt;= 20)" required="true()" constraint="selected(., 'Yes')"/>
      <bind nodeset="/data/plantData/filteredWaterTurbidityCheck" type="select1" relevant="(/data/plantData/filteredWaterTurbidity &gt;= 5)" required="true()" constraint="selected(., 'Yes')"/>
      <bind nodeset="/data/backwash" relevant="(selected(/data/purpose, 'backwash'))"/>
      <bind nodeset="/data/backwash/whichFilter" type="select1" relevant="(selected(/data/purpose, 'backwash')) and (selected(/data/plant, 'San Nicolas') or selected(/data/plant, 'Otoro'))"/>
      <bind nodeset="/data/backwash/backwashTime" type="time" relevant="(selected(/data/purpose, 'backwash'))"/>
      <bind nodeset="/data/maintenanceDone" relevant="(selected(/data/purpose, 'maintenanceDone'))"/>
      <bind nodeset="/data/maintenanceDone/repairsDoneOther" type="string" relevant="(selected(/data/purpose, 'maintenanceDone'))"/>
      <bind nodeset="/data/MaintenanceTODO" relevant="(selected(/data/purpose, 'maintenanceTODO'))"/>
      <bind nodeset="/data/MaintenanceTODO/ToDoDescription" type="string" relevant="(selected(/data/purpose, 'maintenanceTODO'))"/>
      <bind nodeset="/data/comments" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 ref="/data/plant">
      <label ref="jr:itext('/data/plant:label')"/>
      <hint ref="jr:itext('/data/plant:hint')"/>
      <item>
        <label ref="jr:itext('/data/plant:option0')"/>
        <value>Ojojona</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option1')"/>
        <value>Tamara</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option2')"/>
        <value>Marcala</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option3')"/>
        <value>CuatroComunidades</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option4')"/>
        <value>Agalteca</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option5')"/>
        <value>Alauca</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option6')"/>
        <value>Atima</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option7')"/>
        <value>San Nicolas</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option8')"/>
        <value>Moroceli</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option9')"/>
        <value>Otoro</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option10')"/>
        <value>Matias</value>
      </item>
      <item>
        <label ref="jr:itext('/data/plant:option11')"/>
        <value>Other</value>
      </item>
    </select1>
    <select ref="/data/purpose">
      <label ref="jr:itext('/data/purpose:label')"/>
      <item>
        <label ref="jr:itext('/data/purpose:option0')"/>
        <value>plantData</value>
      </item>
      <item>
        <label ref="jr:itext('/data/purpose:option1')"/>
        <value>backwash</value>
      </item>
      <item>
        <label ref="jr:itext('/data/purpose:option2')"/>
        <value>maintenanceDone</value>
      </item>
      <item>
        <label ref="jr:itext('/data/purpose:option3')"/>
        <value>maintenanceTODO</value>
      </item>
      <item>
        <label ref="jr:itext('/data/purpose:option4')"/>
        <value>Other</value>
      </item>
    </select>
    <group appearance="field-list">
      <label ref="jr:itext('/data/plantData:label')"/>
      <input ref="/data/plantData/timeCollected">
        <label ref="jr:itext('/data/plantData/timeCollected:label')"/>
        <hint ref="jr:itext('/data/plantData/timeCollected:hint')"/>
      </input>
      <input ref="/data/plantData/dateCollected" appearance='no-calendar'>
        <label ref="jr:itext('/data/plantData/dateCollected:label')"/>
        <hint ref="jr:itext('/data/plantData/dateCollected:hint')"/>
      </input>
      <input ref="/data/plantData/flowRate">
        <label ref="jr:itext('/data/plantData/flowRate:label')"/>
        <hint ref="jr:itext('/data/plantData/flowRate:hint')"/>
      </input>
      <select1 ref="/data/plantData/flowUnits">
        <label ref="jr:itext('/data/plantData/flowUnits:label')"/>
        <item>
          <label ref="jr:itext('/data/plantData/flowUnits:option0')"/>
          <value>Lps</value>
        </item>
        <item>
          <label ref="jr:itext('/data/plantData/flowUnits:option1')"/>
          <value>Galpmin</value>
        </item>
      </select1>
      <input ref="/data/plantData/coagulantDose">
        <label ref="jr:itext('/data/plantData/coagulantDose:label')"/>
        <hint ref="jr:itext('/data/plantData/coagulantDose:hint')"/>
      </input>
      <input ref="/data/plantData/CDCPos">
        <label ref="jr:itext('/data/plantData/CDCPos:label')"/>
        <hint ref="jr:itext('/data/plantData/CDCPos:hint')"/>
      </input>
      <input ref="/data/plantData/rawWaterTurbidity">
        <label ref="jr:itext('/data/plantData/rawWaterTurbidity:label')"/>
        <hint ref="jr:itext('/data/plantData/rawWaterTurbidity:warning')"/>
      </input>
      <input ref="/data/plantData/settledWaterTurbidity">
        <label ref="jr:itext('/data/plantData/settledWaterTurbidity:label')"/>
        <hint ref="jr:itext('/data/plantData/settledWaterTurbidity:hint')"/>
      </input>
      <input ref="/data/plantData/filteredWaterTurbidity">
        <label ref="jr:itext('/data/plantData/filteredWaterTurbidity:label')"/>
        <hint ref="jr:itext('/data/plantData/filteredWaterTurbidity:hint')"/>
      </input>
      <input ref="/data/plantData/chlorineDose">
        <label ref="jr:itext('/data/plantData/chlorineDose:label')"/>
        <hint ref="jr:itext('/data/plantData/chlorineDose:hint')"/>
      </input>
      <input ref="/data/plantData/entranceWaterLevel">
        <label ref="jr:itext('/data/plantData/entranceWaterLevel:label')"/>
      </input>
    </group>
    <!-- change -->
    <group ref="/data/consented" appearance="field-list" 
            >
      <input ref="/data/consented/coagDoseSuggestion" appearance="ex:org.post.aguaclara.postdose.COLLECT(rawWaterTurbidity=/data/plantData/rawWaterTurbidity)"> <!-- Need to pass the raw water turbidity for now -->
        <label ref="jr:itext('/data/plantData/coagDoseSuggestion:label')"/>
      </input>
      <input ref="/data/plantData/coagulantDose">
        <label ref="jr:itext('/data/plantData/coagulantDose:label')"/>
        <hint ref="jr:itext('/data/plantData/coagulantDose:hint')"/>
      </input>
    </group>
    <!-- /change -->
    <group appearance="field-list">
      <select1 ref="/data/plantData/coagulantCheck">
        <label ref="jr:itext('/data/plantData/coagulantCheck:label')"/>
        <hint ref="jr:itext('/data/plantData/coagulantCheck:hint')"/>
        <item>
          <label ref="jr:itext('/data/plantData/coagulantCheck:option0')"/>
          <value>Yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/plantData/coagulantCheck:option1')"/>
          <value>No</value>
        </item>
      </select1>
      <select1 ref="/data/plantData/settledWaterTurbidityCheck">
        <label ref="jr:itext('/data/plantData/settledWaterTurbidityCheck:label')"/>
        <hint ref="jr:itext('/data/plantData/settledWaterTurbidityCheck:hint')"/>
        <item>
          <label ref="jr:itext('/data/plantData/settledWaterTurbidityCheck:option0')"/>
          <value>Yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/plantData/settledWaterTurbidityCheck:option1')"/>
          <value>No</value>
        </item>
      </select1>
      <select1 ref="/data/plantData/filteredWaterTurbidityCheck">
        <label ref="jr:itext('/data/plantData/filteredWaterTurbidityCheck:label')"/>
        <hint ref="jr:itext('/data/plantData/filteredWaterTurbidityCheck:hint')"/>
        <item>
          <label ref="jr:itext('/data/plantData/filteredWaterTurbidityCheck:option0')"/>
          <value>Yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/plantData/filteredWaterTurbidityCheck:option1')"/>
          <value>No</value>
        </item>
      </select1>
    </group>
    <select1 ref="/data/plantData/backwashed">
        <label ref="jr:itext('/data/plantData/backwashed:label')"/>
        <hint ref="jr:itext('/data/plantData/backwashed:hint')"/>
        <item>
          <label ref="jr:itext('/data/plantData/backwashed:option0')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/plantData/backwashed:option1')"/>
          <value>no</value>
        </item>
      </select1>
    <group appearance="field-list">
      <input ref="/data/plantData/timeBackwashed">
        <label ref="jr:itext('/data/plantData/timeBackwashed:label')"/>
        <hint ref="jr:itext('/data/plantData/timeBackwashed:hint')"/>
      </input>
      <input ref="/data/plantData/dateBackwashed" appearance='no-calendar'>
        <label ref="jr:itext('/data/plantData/dateBackwashed:label')"/>
        <hint ref="jr:itext('/data/plantData/dateBackwashed:hint')"/>
      </input>
    </group>
    <group>
      <label ref="jr:itext('/data/backwash:label')"/>
      <select1 ref="/data/backwash/whichFilter">
        <label ref="jr:itext('/data/backwash/whichFilter:label')"/>
        <item>
          <label ref="jr:itext('/data/backwash/whichFilter:option0')"/>
          <value>1</value>
        </item>
        <item>
          <label ref="jr:itext('/data/backwash/whichFilter:option1')"/>
          <value>2</value>
        </item>
      </select1>
      <input ref="/data/backwash/backwashTime">
        <label ref="jr:itext('/data/backwash/backwashTime:label')"/>
        <hint ref="jr:itext('/data/backwash/backwashTime:hint')"/>
      </input>
    </group>
    <group>
      <label ref="jr:itext('/data/maintenanceDone:label')"/>
      <input ref="/data/maintenanceDone/repairsDoneOther">
        <label ref="jr:itext('/data/maintenanceDone/repairsDoneOther:label')"/>
      </input>
    </group>
    <group>
      <label ref="jr:itext('/data/MaintenanceTODO:label')"/>
      <input ref="/data/MaintenanceTODO/ToDoDescription">
        <label ref="jr:itext('/data/MaintenanceTODO/ToDoDescription:label')"/>
      </input>
    </group>
    <input ref="/data/comments">
      <label ref="jr:itext('/data/comments:label')"/>
      <hint ref="jr:itext('/data/comments:hint')"/>
    </input>
  </h:body>
</h:html>
